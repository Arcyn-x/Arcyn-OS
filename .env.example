# Arcyn OS Configuration
# Copy this to .env and fill in your values

# =============================================================================
# LLM PROVIDER (Required for intelligent agents)
# =============================================================================

# Google Gemini API Key
# Get your key from: https://aistudio.google.com/app/apikey
GEMINI_API_KEY=your-gemini-api-key-here

# Gemini Model (optional, defaults to gemini-2.5-flash)
# Options:
#   Gemini 3 (Preview): gemini-3-pro-preview, gemini-3-flash-preview
#   Gemini 2.5 (Stable): gemini-2.5-flash, gemini-2.5-pro, gemini-2.5-flash-lite
#   Gemini 2.0: gemini-2.0-flash, gemini-2.0-flash-lite
#   Latest: gemini-flash-latest, gemini-pro-latest
GEMINI_MODEL=gemini-2.5-flash

# =============================================================================
# ALTERNATIVE PROVIDERS (Future support)
# =============================================================================

# OpenAI (not yet implemented)
# OPENAI_API_KEY=

# Anthropic (not yet implemented)
# ANTHROPIC_API_KEY=

# =============================================================================
# ARCYN OS SETTINGS
# =============================================================================

# Log level: DEBUG, INFO, WARNING, ERROR
LOG_LEVEL=INFO

# Memory storage path
MEMORY_PATH=./memory

# Knowledge database path
KNOWLEDGE_DB_PATH=./knowledge/knowledge.db
